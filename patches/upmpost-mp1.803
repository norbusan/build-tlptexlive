+upsvgout-tangle: ctangle$(EXEEXT) mplibdir/svgout.w upsvgout-tot.ch tangle-sh
+	cat $(srcdir)/mplibdir/svgout.w | $(upmp_sed) > upsvgout.w
+	@$(upmp_ctangle_sh) upsvgout upsvgout-tot.ch
+upsvgout-tot.ch: ctie$(EXEEXT) mplibdir/svgout.w pmpostdir/svgout.ch upmpostdir/upsvgout.ch
+	cat $(srcdir)/mplibdir/svgout.w | $(upmp_sed) > upsvgout.w
+	$(ctie) -c upsvgout-tot.ch upsvgout.w pmpostdir/svgout.ch upmpostdir/upsvgout.ch
+
+$(uppngout_c_h): uppngout-tangle
+	@$(upmp_ctangle_sh) uppngout
+uppngout-tangle: ctangle$(EXEEXT) mplibdir/pngout.w tangle-sh
+	cat $(srcdir)/mplibdir/pngout.w | $(upmp_sed) > uppngout.w
