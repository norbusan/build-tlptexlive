 if test "x$cross_compiling" = xyes; then :
+  # Extract the first word of "ctie", so it can be a program name with args.
+set dummy ctie; ac_word=$2
+{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
+$as_echo_n "checking for $ac_word... " >&6; }
+if ${ac_cv_path_CTIE+:} false; then :
+  $as_echo_n "(cached) " >&6
+else
+  case $CTIE in
+  [\\/]* | ?:[\\/]*)
+  ac_cv_path_CTIE="$CTIE" # Let the user override the test with a path.
+  ;;
+  *)
+  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
+for as_dir in $PATH
+do
+  IFS=$as_save_IFS
+  test -z "$as_dir" && as_dir=.
+    for ac_exec_ext in '' $ac_executable_extensions; do
+  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
+    ac_cv_path_CTIE="$as_dir/$ac_word$ac_exec_ext"
+    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
+    break 2
+  fi
+done
+  done
+IFS=$as_save_IFS
+
+  ;;
+esac
+fi
+CTIE=$ac_cv_path_CTIE
+if test -n "$CTIE"; then
+  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $CTIE" >&5
+$as_echo "$CTIE" >&6; }
+else
+  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
+$as_echo "no" >&6; }
+fi
+
+
+if test -z "${CTIE}"; then :
+  as_fn_error $? "ctie was not found but is required when cross-compiling.
+  Install ctie or set \$CTIE to the full pathname." "$LINENO" 5
+fi
+else
+  CTIE=./ctie
+fi
+
+if test "x$cross_compiling" = xyes; then :
   # Extract the first word of "otangle", so it can be a program name with args.
