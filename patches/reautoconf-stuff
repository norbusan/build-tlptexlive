+# Check whether --enable-pmp was given.
+if test "${enable_pmp+set}" = set; then :
+  enableval=$enable_pmp;
+fi
+case $enable_pmp in #(
+  yes | no) :
+     ;; #(
+  *) :
+    enable_pmp=yes ;;
+esac
+
+test "x$enable_web2c:$enable_pmp" = xyes:yes && {
+  need_cairo=yes
+  need_libpng=yes
+  need_ptexenc=yes
