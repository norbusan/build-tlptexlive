+      bbox_valid = True;
+      bbox_width = hsize * dimconv / currwin.shrinkfactor + 0.5;
+      bbox_voffset = bbox_height = vsize * dimconv / currwin.shrinkfactor + 0.5;
+
+      if (currwin.win == mane.win) {
+#if   PS
+          psp.drawbegin(PXL_H - currwin.base_x, PXL_V - currwin.base_y,
+              buffer);
