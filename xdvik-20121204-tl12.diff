+static  char *
+DirName(f1, f2)
+        char    *f1;
+        char    *f2;
+{
+        char    *p1, *p2, *q1;
+        static char     buf[BUFSIZ];
+
+#ifndef VMS
+        if (*f2 == '/')
+                return f2;
+#endif  /* !VMS */
+
+        for (p1 = p2 = f1, q1 = buf; *p1; p1++, q1++) {
+#ifndef VMS
+            if (*p1 == '/')
+#else
+            if (*p1 == ':')
+#endif  /* !VMS */
+                p2 = p1 + 1;
+            *q1 = *p1;
+        }
+        strcpy(buf + (p2 - f1), f2);
+
+        return buf;
