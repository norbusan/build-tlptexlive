+/* JIS (ku-ten) -> unicode */
+int jis2uni(int jis)
+{
+  int ku, ten;
+
+  ku = (jis >> 8) - 0x20;
+  if (ku < 1 || ku > MAXJIS) return (0);
+  ten = (jis % 256) - 0x20;
+  if (ten < 1 || ten > 94) return (0);
+  return ((int)jis2uni_table[ku - 1][ten - 1]);
+}
